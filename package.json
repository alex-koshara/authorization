{
  "name": "dashboard-api",
  "version": "1.0.0",
  "description": "Learning API",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "prestart": "npx kill-port 8000",
    "generate": "prisma generate",
    "dev": "nodemon",
    "dev:inspect": "nodemon -e ts,json --exec node --inspect=localhost:9222 -r ts-node/register src/main.ts",
    "kill": "npx kill-port 8000",
    "lint": "eslint ./src/**",
    "lint:fix": "eslint ./src/** --fix",
    "start": "nodemon ./dist/main.js",
    "build": "tsc",
    "test": "jest",
    "test:e2e": "npm run kill && jest --config jest.e2e.config.ts --coverage"
  },
  "author": "Alex Koshara",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.10.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "inversify": "^6.0.2",
    "jsonwebtoken": "^9.0.2",
    "reflect-metadata": "^0.2.1",
    "tslog": "^4.9.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "prisma": "^5.10.2",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
